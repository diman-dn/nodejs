<h1>Ваша корзина</h1>
{{#each cart.errors}}
    <div class="alert alert-danger">{{.}}</div>
{{/each}}
{{#each cart.warnings}}
    <div class="alert alert-warning">{{.}}</div>
{{/each}}

<table class="table table-striped">
    <thead>
    <tr>
        <td>Туры</td>
        <td>Гости</td>
    </tr>
    </thead>
    <tbody>
    {{#each cart.items}}
        <tr>
            <td>{{vacation.name}}</td>
            <td>{{guests}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>

<a class="btn btn-success" href="/cart/checkout">Оформить</a>